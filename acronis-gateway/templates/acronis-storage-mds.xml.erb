<?xml version="1.0" encoding="utf-8"?>
<ArchivingStorageSystemConfiguration>
  <MetadataServer>
    <ThreadCount>8</ThreadCount>
    <NetworkOptions>
      <ThreadCount>8</ThreadCount>
      <InterruptTimeout><Milliseconds>100</Milliseconds></InterruptTimeout>
    </NetworkOptions>
    <ListenInterface>127.0.0.1:4444</ListenInterface>
    <StorageRegistrationInterface>127.0.0.1:4444</StorageRegistrationInterface>
    <RequestTimeout><Seconds>900</Seconds></RequestTimeout>
    <KeepConnectionTime><Seconds>600</Seconds></KeepConnectionTime>
    <MetadataServers/>
    <LocksTimeout><Minutes>10</Minutes></LocksTimeout>
    <AllocateChunksCountValue>129</AllocateChunksCountValue>
    <GarbageRemoveDelay><Hours>3</Hours></GarbageRemoveDelay>
    <RestorationsPerServerLimit>4</RestorationsPerServerLimit>
    <ChunkRestorationDelays>
      <Item><Minutes>1</Minutes></Item>
      <Item><Minutes>5</Minutes></Item>
      <Item><Hours>1</Hours></Item>
    </ChunkRestorationDelays>
    <DataBalancer>
      <Sensitivity>0.234</Sensitivity>
      <Latency><Seconds>30</Seconds></Latency>
      <SimultaneousMovements>30</SimultaneousMovements>
    </DataBalancer>
    <TransactionsJournal>
      <JournalFile>/var/lib/Acronis/acronis-storage-mds/journal</JournalFile>
      <SnapshotFile>/var/lib/Acronis/acronis-storage-mds/snapshot</SnapshotFile>
      <FlushTime><Milliseconds>10</Milliseconds></FlushTime>
      <FileSizeBase>10485760</FileSizeBase>
    </TransactionsJournal>
    <TransactionQueue>
      <InquireTimeout><Seconds>2</Seconds></InquireTimeout>
      <FarFlushDistance>4026593280</FarFlushDistance>
    </TransactionQueue>
    <Redundancy>
      <RoundtripTimeout><Seconds>80</Seconds></RoundtripTimeout>
      <InitialStateTimeout><Seconds>200</Seconds></InitialStateTimeout>
      <ResetStateTimeout><Seconds>80</Seconds></ResetStateTimeout>
      <SendPeriod><Seconds>5</Seconds></SendPeriod>
    </Redundancy>
    <ChunksTableUpdatePeriod><Seconds>5</Seconds></ChunksTableUpdatePeriod>
    <DisksDiagnostics>
      <ControlPeriod>
        <Seconds>180</Seconds>
      </ControlPeriod>
      <ControlPeriodsCount>5</ControlPeriodsCount>
      <RatingThreshold>6</RatingThreshold>
      <MetaOperationThreshold>480</MetaOperationThreshold>
      <RWRateThreshold>2000</RWRateThreshold>
    </DisksDiagnostics>
    <Statistic>
      <Filter><Last/></Filter>
      <DumpFileName>/var/log/Acronis/acronis-storage-mds/stat.txt</DumpFileName>
      <DumpPeriod><Seconds>10</Seconds></DumpPeriod>
    </Statistic>
    <LoggingSettings>
      <Binary>
        <LogFilePrefix>/var/log/Acronis/acronis-storage-mds/</LogFilePrefix>
        <LogFileSizeLimit>128</LogFileSizeLimit>
        <Filter>
          <Severity>
            <Normal>Info</Normal>
            <Errors>Warning</Errors>
          </Severity>
        </Filter>
        <RetentionPolicy>
          <Limits>
            <Size>51200</Size>
          </Limits>
        </RetentionPolicy>
      </Binary>
    </LoggingSettings>
  </MetadataServer>
</ArchivingStorageSystemConfiguration>
